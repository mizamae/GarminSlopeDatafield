<settings>	

	<setting propertyKey="@Properties.LSREGRESSION_SAMPLES" title="@Strings.LSREGRESSION_SAMPLES">
    	<settingConfig type="numeric" min="2" max="50" />
	</setting>
	
	<setting propertyKey="@Properties.ALTITUDE_FILTER_COEFF" title="@Strings.ALTITUDE_FILTER_COEFF">
    	<settingConfig type="numeric" min="0" max="99" />
	</setting>
	
	<setting propertyKey="@Properties.RISE_CALCULATION" title="@Strings.RISE_CALCULATION">
		<settingConfig type="list">
 			<listEntry value="0">@Strings.SlopePC</listEntry>
 			<listEntry value="1">@Strings.SlopeDEG</listEntry>
 		</settingConfig>
 	</setting>

</settings>